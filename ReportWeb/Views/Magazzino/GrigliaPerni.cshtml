
@using ReportWeb.Models.Magazzino
@model  List<PernioModel>



@if (Model.Count == 0)
{
    <h3 class="text-red">Nessun elemento trovato</h3>
}
else
{
    <table class="table table-condensed" id="tblCampionario">
        <thead>
            <tr>
                <th>ID</th>
                <th>Articolo</th>
                <th>Cliente</th>
                <th>Posizione</th>
                <th>Componente</th>
                <th>Cod. Interno</th>
                <th>Prog. Stampo</th>
                <th>Descrizione</th>
                <th>Diametro</th>
                <th>Lunghezza</th>
                <th>Quantita</th>
                <th>Giacenza</th>
            </tr>
        </thead>
        <tbody>
            @foreach (PernioModel elemento in Model)
            {
            <tr>
                <td>@elemento.IdPosizPerno</td>
                <td>@elemento.Articolo</td>
                <td>@elemento.Cliente</td>
                <td>@elemento.Posizione</td>
                <td>@elemento.Componente</td>
                <td>@elemento.CodiceInterno</td>
                <td>@elemento.ProgressivoStampo</td>
                <td>@elemento.Descrizione</td>
                <td>@elemento.Diametro</td>
                <td>@elemento.Lunghezza</td>
                <td>@elemento.Quantita</td>
                <td>@elemento.GiacenzaMinima</td>
            </tr>
            }
        </tbody>
    </table>
}


