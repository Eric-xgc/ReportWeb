@using ReportWeb.Models.Magazzino
@model  List<MagazzinoCampionarioModel>



@if (Model.Count == 0)
{
    <h3 class="text-red">Nessun elemento trovato</h3>
}
else
{
    <table class="table table-condensed" id="tblCampionario">
        <thead>
            <tr>
                <th>Id</th>
                <th>Codice</th>
                <th>Finitura</th>
                <th>Piano</th>
                <th>Posizione</th>
                <th>Descrizione</th>
            </tr>
        </thead>
        <tbody>
            @foreach (MagazzinoCampionarioModel elemento in Model)
            {
                <tr>
                    <td>@elemento.IDMAGAZCAMP</td>
                    <td>@elemento.Codice</td>
                    <td>@elemento.Finitura</td>
                    <td>@elemento.Piano</td>
                    <td>@elemento.Posizione</td>
                    <td>@elemento.Descrizione</td>
                </tr>
            }
        </tbody>
    </table>
}

   