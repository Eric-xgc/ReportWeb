@model List<string>
@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_LayoutApplication.cshtml";
}


<div class="content-top">
    <h2>Metal Web Home Page</h2>
    @if (Model.Count > 0)
    {
        <div class="box">
            <div class="box-body form-group">
                <ul class="form-control">
                    @foreach (string messaggio in Model)
                    {
                        <li>
                            @messaggio
                        </li>
                    }
                </ul>

            </div>
        </div>
    }




</div>
